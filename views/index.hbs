<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>


<p>Here is a list of projects and their repos:</p>
{{#each allProjects as |project|}}
  <section>
    <a href="{{project.repo}}" target="_blank" alt="{{project.name}}" title="{{project.name}}"><img src="{{project.imageFile}}"></a>
    <h2>{{project.name}}</h2>
    <ul>
      {{#each project.technologies as |technology|}}
        <li>{{technology}}</li>
      {{/each}} <!-- technology -->
    </ul>
    <a href="{{project.repo}}" target="_blank" alt="{{project.name}}" title="{{project.name}}" class="repo-link">repo</a>
  </section>
{{/each}} <!-- project -->
